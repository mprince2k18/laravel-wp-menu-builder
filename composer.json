{
    "name": "mprince2k18/laravel-wp-menu-builder",
    "description": "drag and drop menu generator like wordpress for laravel 9",
    "keywords": ["Laravel", "Menu", "Wordpress", "Builder"],
    "license": "MIT",
    "authors": [
    	{
            "name": "Mohammad Prince",
            "email": "mprince2k16@gmail.com",
            "role": "Senior Software Engineer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "5.* || 6.* || 7.* || 8.*"
    },
    "autoload": {
        "psr-4": {
          "Mprince\\Menu\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mprince\\Menu\\MenuServiceProvider"
            ],
            "aliases": {
                "Menu": "Mprince\\Menu\\Facades\\Menu"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
